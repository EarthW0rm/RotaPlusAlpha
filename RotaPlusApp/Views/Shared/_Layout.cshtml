@using System.Web.Optimization;

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="pt-br">
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    
    @Styles.Render("~/StylesBundle/Plugins")

    <title>Calculadora de Rotas - RotaPlus</title>

</head>
<body class="skin-blue sidebar-mini wysihtml5-supported" ng-app="rotaplus">
    <div class="wrapper">
        <header class="main-header">
            <a href="#" class="logo">
                <span class="logo-mini"><b>C</b>Rota</span>
                <span class="logo-lg"><b>Rota</b>Plus</span>
            </a>
            <nav class="navbar navbar-static-top" role="navigation">
                <div class="navbar-custom-menu">
                    <span style="padding-top: 12px;padding-right: 12px;display: block;color: white;float: right;">
                        <img src="~/Content/img/powered_by_google_on_non_white.png" alt="powered by Google" />
                    </span>
                </div>
               
            </nav>
        </header>

        <div class="content-wrapper">
            <div class="container-fluid no-padding">
                @RenderBody()
            </div>
        </div>
    </div>

    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDpfg4bfsWD_0x3MWm1vmP_PHuFadeXPQs&libraries=places"></script>
    @Scripts.Render("~/ScriptsBundle/Plugins")
    @Scripts.Render("~/ScriptsBundle/GMap")
    @Scripts.Render("~/ScriptsBundle/RotaPlus")

    @RenderSection("Scripts")

</body>
</html>
